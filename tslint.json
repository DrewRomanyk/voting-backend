{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "jsx-boolean-value": false,
        "object-literal-sort-keys": false,
        "no-unused-variable": true,
        "typedef": [
            true,
            "call-signature",
            "property-declaration"
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    }
}
